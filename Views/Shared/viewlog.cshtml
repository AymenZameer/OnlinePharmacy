@{
    ViewBag.Title = "viewlog";
   
}


<!DOCTYPE html>
<html>
<head>
    @using OnlinePharmacy.Models;
   @Scripts.Render("~/bundles/jquery")
@RenderSection("scripts", required: false)
   
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
   
    <script src="~/Scripts/jquery-1.10.1.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />

    <script src="~/Scripts/jquery.fancybox.js"></script>
    <script src="~/Scripts/jquery.fancybox.pack.js"></script>
    



  


    
    
    <link href="~/Content/Site.css" rel="stylesheet" />
 
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>


   
    
    <style>
        header {
            position: absolute;
            width: 100%;
            height: 100%;
            background-image: url(../../Content/themes/base/images/ack1.jpg);
            background-position: center;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            background-size: cover;
            -o-background-size: cover;
            opacity: 0.2;
            
        }

        .navbar .nav-pills > li > a {
            color: #FA8072;
        }

        .nav-pills {
            font-family: Book Antiqua;
            font-size: 20px;
        }

        .control-label {
            font-family: Book Antiqua;
            font-size: 20px;
        }

        .navbar-nav {
            font-family: Book Antiqua;
            font-size: 20px;
        }
    </style>
    
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    
    <nav class="navbar navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"></button>
                <a class="navbar-nav" href="/LoggedIn/Index"><img src="~/Content/themes/base/images/logo.png"></a>
            </div>
            <div class="collapse navbar-collapse">

                <form class="navbar-form navbar-left" role="search" action="/LoggedIn/Search">
                    <div class="form-group">
                        <input class="form-control" type="text" data-autocomplete-source="@Url.Action("QuickSearch","LoggedIn")" placeholder="Search " style="width:380px" name="search"  />
                    </div>
                    <button type="submit" class="btn btn-danger" onclick="searching()" style="font-family:'Book Antiqua'">Search</button>
                </form>
                <ul class="nav nav-pills pull-right">
                    <li>
                        <a href="/LoggedIn/Statuses">Orders</a>
                    </li>
                    <li>
                        <a href="/LoggedIn/Suggestions">Suggestions</a>
                    </li>
                            <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">@ViewBag.User.name<span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="/LoggedIn/profileInfo">Profile info</a></li>
                            <li><a href="/LoggedIn/AboutUs">AboutUs</a></li>
                            <li><a href="/LoggedIn/ContactUs">ContactUs</a></li>
                            <li class="divider"></li>
                            <li><a href="/LoggedIn/SignOut">SignOut</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    
    <script>
        function searching()
        {
            var a = $('#search').val();

            if (a != null)
            {
                $('#ghi').empty();
                $('#ghi').load("/LoggedIn/Search?str="+a);
            }
            
        }
       


    </script>
   
    <script>


        $("input[data-autocomplete-source]").each(function () {
            var target = $(this);
            target.autocomplete({ source: target.attr("data-autocomplete-source") });
        });
    </script>

    @RenderBody()

    

</body>
</html>





